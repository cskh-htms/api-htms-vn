

<% if( typeof news_list == 'object' && news_list){ } else{ news_list=[] } %>

<table class="table-list">
	<thead>
		<tr>
			<th class="user-name">Tiêu đề</th>
			<th class="user-name">Danh mục</th>
			<th class="user-name">Người đăng</th>
			<th class="user-name">Ngày đăng</th>
			<th class="user-name">Trạng thái</th>
		</tr>
	</thead>
	<tbody>

	<% 
	for( var  x in news_list ) { 
	%>

	   <tr>
			<td class="user-name"><a href="#"><%- news_list[x].news_title %></a>
				<div class="row-actions">
					<span class="edit"><a href="/news/general/show/<%= news_list[x].news_ID  %>">Chỉnh sửa</a> | </span>
					<span class="delete"><a href="#" data_news_id="<%= news_list[x].news_ID  %>" class="submitdelete">Xóa</a></span>
				</div>
			</td>
			<td class="user-name"><%= news_list[x].category_news_name %></td>	
			<td class="user-name"><%= news_list[x].users_first_name + " " + news_list[x].users_last_name %></td>				
			<td class="user-name"><%= news_list[x].news_date_created %></td>
			<td class="user-name"><%- ojs_share.show_table_status_product(news_list[x].news_status ) %></td>				
			
	   </tr>


	<% } %>
	
	</tbody>
	<tfoot>
		<tr>
			<th class="user-name">Tiêu đề</th>
			<th class="user-name">Danh mục</th>
			<th class="user-name">Người đăng</th>
			<th class="user-name">Ngày đăng</th>
			<th class="user-name">Trạng thái</th>
		</tr>
	</tfoot>
</table>