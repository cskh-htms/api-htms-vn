		//@
		let user_id = $('#user_id').val();
		let store_id = $('#store_id').val();
		let status_admin = [0,1,2,3]
		
		var datas = {
			"datas":{
				user_id 	: user_id,
				status_admin : status_admin,
				store_id:store_id
			}
		}	
		//console.log(datas);
		//return;
		ojs_bussiness.ajax_load_products(datas);	

	});//end of function		
			
	
	
	//@
	//goi ajax
	$(document).on('click','#complate',function(e) {	
		e.preventDefault();
