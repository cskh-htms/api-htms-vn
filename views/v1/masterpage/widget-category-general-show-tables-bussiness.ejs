

<% if( typeof datas_category_general == 'object' && datas_category_general){ datas_category_general = datas_category_general } else{ datas_category_general=[] } %>





<table class="table-list">
	<thead>
		<tr>
			<th class="user-name">Tên danh mục</th>
			<th class="user-name">trạng thái admin</th>
			<th class="user-name">trạng thái cửa hàng</th>		
		</tr>
	</thead>
	<tbody>


	<% 
	for(var  x in datas_category_general) { %>
		<% if(datas_category_general[x].category_general_speciality_category_parent_id == 0) { %>
		
				<% //nếu chủ cữa hàng thì hiện màu đỏ
				if(datas_category_general[x].users_ID == user_id  &&  user_role != "admin"){ 		
					var ex1 = 'my_store';
				}else{
					var ex1 = '';
				}
				%>				
		
		
			   <tr class="<%= ex1 %>">
					<td class="user-name"><a href="#"><%= datas_category_general[x].category_general_speciality_name %></a>
						<div class="row-actions">
							<% //nếu là admin thì hiển thị update
							if(user_role == "admin" ||  datas_category_general[x].users_ID == user_id){ %>
							
								<% //neu nếu đã chấp nhận thì ko cho update nua
								if( datas_category_general[x].category_general_speciality_update_status != "1"   || user_role == "admin"){ %>							
									<span class="edit"><a href="/categorys/general/speciality/show/<%= datas_category_general[x].category_general_speciality_ID %>/<%= user_id %>">Chỉnh sửa</a> | </span>
									<span class="delete"><a href="#" data_cat_id="<%= datas_category_general[x].category_general_speciality_ID %>" class="submitdelete">Xóa</a></span>
								<% //enf of if
								}
								%>
							<% //enf of if
							}
							%>
							
						</div>
					</td>
				
						<td class="user-name"><%- ojs_share.show_status_table_share_all(datas_category_general[x].category_general_speciality_admin_status) %></td>
						<td class="user-name"><%- ojs_share.show_status_table_share_all(datas_category_general[x].category_general_speciality_stores_status) %></td>
	
			   </tr>
			   
			   <% for(var  y in datas_category_general) { %>
					<% if(datas_category_general[x].category_general_speciality_ID == datas_category_general[y].category_general_speciality_category_parent_id) { %>
					
							<% //nếu chủ cữa hàng thì hiện màu đỏ
							if(datas_category_general[y].users_ID == user_id   &&  user_role != "admin"){ 		
								var ex1 = 'my_store';
							}else{
								var ex1 = '';
							}
							%>		
					
						    <tr  class="<%= ex1 %>">
								<td class="user-name cat_child01" ><a href="#"><%= datas_category_general[y].category_general_speciality_name %></a>
								<div class="row-actions">
									<% //nếu là admin thì hiển thị update
									if(user_role == "admin" || datas_category_general[y].users_ID == user_id){ %>

										<% //neu nếu đã chấp nhận thì ko cho update nua
										if( datas_category_general[y].category_general_speciality_update_status != "1"   || user_role == "admin"){ %>										
											<span class="edit"><a href="/categorys/general/speciality/show/<%= datas_category_general[y].category_general_speciality_ID %>/<%= user_id %>"">Chỉnh sửa</a> | </span>
											<span class="delete"><a  data_cat_id="<%= datas_category_general[y].category_general_speciality_ID %>" href="#" class="submitdelete">Xóa</a></span>
									
										<% //enf of if
										}
										%>										
									<% //enf of if
									}
									%>								
									
								</div>
								</td>
					
									<td class="user-name"><%- ojs_share.show_status_table_share_all(datas_category_general[y].category_general_speciality_admin_status) %></td>
									<td class="user-name"><%- ojs_share.show_status_table_share_all(datas_category_general[y].category_general_speciality_stores_status) %></td>
							
						   </tr>
						   
						   <% for(var  z in datas_category_general) { %>
								<% if(datas_category_general[y].category_general_speciality_ID == datas_category_general[z].category_general_speciality_category_parent_id) { %>
										<% //nếu chủ cữa hàng thì hiện màu đỏ
										if(datas_category_general[z].users_ID == user_id   &&  user_role != "admin"){ 		
											var ex1 = 'my_store';
										}else{
											var ex1 = '';
										}
										%>		
								
										<tr  class="<%= ex1 %>">
											<td class="user-name cat_child02" ><a href="#"><%= datas_category_general[z].category_general_speciality_name %></a>
											<div class="row-actions">
											<% //nếu là admin thì hiển thị update
											if(user_role == "admin" || datas_category_general[z].users_ID == user_id){ %>										

												<% //neu nếu đã chấp nhận thì ko cho update nua
												if( datas_category_general[z].category_general_speciality_update_status != "1"   || user_role == "admin"){ %>										
													<span class="edit"><a href="/categorys/general/speciality/show/<%= datas_category_general[z].category_general_speciality_ID %>/<%= user_id %>"">Chỉnh sửa</a> | </span>
													<span class="delete"><a href="#"  data_cat_id="<%= datas_category_general[z].category_general_speciality_ID %>"  class="submitdelete">Xóa</a></span>
												<% //enf of if
												}
												%>	

											<% //enf of if
											}
											%>												
											
											</div>
											</td>
											
					
												<td class="user-name"><%- ojs_share.show_status_table_share_all(datas_category_general[z].category_general_speciality_admin_status) %></td>
												<td class="user-name"><%- ojs_share.show_status_table_share_all(datas_category_general[z].category_general_speciality_stores_status) %></td>
							
											
											
									   </tr>
								<% } %>	   
							<% } %>						   
				    <% } %>	   
				<% } %>
	   
	   <% } %>
	<% } %>

	</tbody>
	<tfoot>
		<tr>
			<th class="user-name">Tên danh mục</th>
			<th class="user-name">trạng thái admin</th>
			<th class="user-name">trạng thái cửa hàng</th>			
		</tr>
	</tfoot>
</table>
	