<% if( typeof datas == 'object' && datas && datas.length > 0){ datas = datas } else{ datas=[] } %>

	<% 
	if(datas.length > 0){
		
	for( var  x in datas ) { %>
		<tr>
			<td class="user-name"><a href="#"><%- ojs_share.unCape(datas[x].stores_name) %></a>
