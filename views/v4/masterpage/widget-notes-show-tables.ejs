

<% if( typeof datas == 'object' && datas && datas.length > 0){ } else{ datas=[] } %>


<% datas = list_notes %>




<div class="note-box">

<table class="table-list">

	<thead>
		<tr>
			<th class="user-name">Tiêu đề</th>
			<th>Ngày gửi</th>
			<th>Đã xem</th>		
		</tr>
	</thead>
	<tbody>
<% 
if(datas.length > 0){
for(var  x in datas) { %>
	<tr>
		<td class="user-name"><a href="/notes/show/<%= store_id %>/<%= datas[x].notes_ID %>/"><%- datas[x].notes_title -%></a></td>
		<td><%= datas[x].notes_date_created %></td>
		<td><% 
		if(datas[x].notes_status == '0'){ %>
			<span class="news_note">chưa xem</span>			
		<%	
		}
		%></td>
	</tr>

<%
}
}
%>

</tbody>
</table>


</div>