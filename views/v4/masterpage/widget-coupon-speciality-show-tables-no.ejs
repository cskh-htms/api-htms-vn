<% if( typeof coupon_list == 'object' && coupon_list){ } else{ coupon_list=[] } %>

<table class="table-list">
	<thead>
		<tr>
			<th class="user-name">Mã code</th>
			<th>Cửa hàng tạo</th>
			<th>Ngày bắt đầu</th>
			<th>Ngày kết thúc</th>
			<th>Tên công thức</th>
			<th>Trạng thái</th>
		</tr>
	</thead>
	<tbody>
	

	<% 
	for( var  x in coupon_list ) { 
	%>

	   <tr>
			<td class="user-name"><a href="#"><%- coupon_list[x].coupon_speciality_code %></a>
				<%
				//if(
				//coupon_list[x].coupon_speciality_status_admin != "4" && 
				//coupon_list[x].coupon_speciality_status_admin != "1"   
				//){
				%>
				
					<div class="row-actions">
						<span class="edit"><a href="/coupon/speciality/show/<%= coupon_list[x].coupon_speciality_ID  %>/<%= coupon_list[x].stores_ID  %>">Chỉnh sửa</a> | </span>
						<span class="delete"><a href="#" data_coupon_id="<%= coupon_list[x].coupon_speciality_ID  %>" class="submitdelete">Xóa</a></span>
					</div>
				
				<%
				//}
				%>
			</td>
			<td><%= coupon_list[x].stores_name %></td>	
			<td><%- ojs_share_all.format_date(coupon_list[x].coupon_speciality_date_star) %></td>				
			<td><%- ojs_share_all.format_date(coupon_list[x].coupon_speciality_date_end) %></td>	
			<td><%- ojs_share.show_coupon_formula_table(coupon_list[x].coupon_speciality_formula) %></td>	
			<td><%- ojs_share_coupon.show_status_coupon_html(coupon_list[x].coupon_speciality_status_admin) %></td>			
					

	   </tr>



	<% } %>
	
	</tbody>
	<tfoot>
		<tr>
			<th class="user-name">Mã code</th>
			<th>Cửa hàng tạo</th>
			<th>Ngày bắt đầu</th>
			<th>Ngày kết thúc</th>
			<th>Tên công thức</th>
			<th>Trạng thái</th>		
		</tr>
	</tfoot>
</table>