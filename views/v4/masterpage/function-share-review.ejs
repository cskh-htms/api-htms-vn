	//@ [show_status_review_html]
	show_status_review_html: function(status){
		//
		let result = "";
		//
		if(status == -1){
			result = "" +
			'<i class="fas fa-minus-circle gray" title="Bản nháp"></i>'
			
		}else if(status == 0){
			result = "" + 
			'<i class="fas fa-check-circle yellow" title="Chờ duyệt"></i>'			
			
		}else if(status == 1){
			result = "" + 
			'<i class="fas fa-check-circle green" title="Đã duyệt"></i>'
		}else if(status == 2){
			result = "" + 
			'<i class="fas fa-minus-circle grey" title="Sửa chờ duyệt"></i>'
		}else if(status == 3){
			result = "" + 
			'<i class="fas fa-times-circle dark_red" title="Từ chối"></i>'
		}else if(status == 4){
			result = "" + 
			'<i class="fas fa-check-circle dark_red" style="color:purple;" title="Đã thanh toán"></i>'
		}	

		//	
		return result;	
	},		
	
	
	
	

	
