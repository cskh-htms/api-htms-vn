

<table class="table-list">
	<thead>
		<tr>
			<th class="user-name">tên khu vực khu vực</th>
			<th class="user-name">Giá shipping</th>
			<th class="user-name">show</th>
		</tr>
	</thead>
	<tbody>

	<% 
	for(var  x in datas) { %>
		<% if(datas[x].shipping_speciality_parent_id == 0) { %>
			   <tr style="background-color:#fbf4f4;">
					<td class="user-name"><a href="#"><%= datas[x].shipping_speciality_name %></a>
						<div class="row-actions">
							<span class="edit"><a href="/shipping/speciality/show/<%= datas[x].shipping_speciality_ID %>">Chỉnh sửa</a> | </span>
							<span class="delete"><a href="#" data_shipping_id="<%= datas[x].shipping_speciality_ID %>" class="submitdelete">Xóa</a></span>
						</div>
					</td>
					<td class="user-name"><%= datas[x].shipping_speciality_price %></td>	
					<td class="user-name"><%- ojs_share_all.show_hide_html(datas[x].shipping_speciality_show) %></td>					
			   </tr>
			   
			   <% for(var  y in datas) { %>
					<% if(datas[x].shipping_speciality_ID == datas[y].shipping_speciality_parent_id) { %>
						    <tr>
								<td class="child-01 cat_child01" ><a href="#"><%= datas[y].shipping_speciality_name %></a>
								<div class="row-actions">
									<span class="edit"><a href="/shipping/speciality/show/<%= datas[y].shipping_speciality_ID %>">Chỉnh sửa</a> | </span>
									<span class="delete"><a  data_shipping_id="<%= datas[y].shipping_speciality_ID %>" href="#" class="submitdelete">Xóa</a></span>
								</div>
								</td>
								<td class="child-01 user-name"><%= datas[y].shipping_speciality_price %></td>	
								<td class="child-01 user-name"><%- ojs_share_all.show_hide_html(datas[y].shipping_speciality_show) %></td>	
								
								
						   </tr>
						   
						   <% for(var  z in datas) { %>
								<% if(datas[y].shipping_speciality_ID == datas[z].shipping_speciality_parent_id) { %>
									   <tr>
											<td class="child-02 user-name cat_child02"><a href="#"><%= datas[z].shipping_speciality_name %></a>
											<div class="row-actions">
												<span class="edit"><a href="/shipping/speciality/show/<%= datas[z].shipping_speciality_ID %>">Chỉnh sửa</a> | </span>
												<span class="delete"><a href="#"  data_shipping_id="<%= datas[z].shipping_speciality_ID %>"  class="submitdelete">Xóa</a></span>
											</div>
											</td>
											
											<td class="child-02 user-name"><%= datas[z].shipping_speciality_price %></td>	
											<td class="child-02 user-name"><%- ojs_share_all.show_hide_html(datas[z].shipping_speciality_show) %></td>
									   </tr>
								<% } %>	   
							<% } %>						   
				    <% } %>	   
				<% } %>
	   
	   <% } %>
	<% } %>

	</tbody>
	<tfoot>
		<tr>
			<th class="user-name">Tên khu vực</th>
			<th class="user-name">Giá shipping</th>
			<th class="user-name">show</th>
		</tr>
	</tfoot>
</table>
	