<% 
for(var  x in datas) { 
%>
	<% 
	if(datas[x].shipping_speciality_parent_id == 0) { 
	%>
		<option value ="<%= datas[x].shipping_speciality_ID %>"><%= datas[x].shipping_speciality_name %></option>
		
		<%
		for(var  y in datas) {
		%>
			<% 
			if(datas[x].shipping_speciality_ID == datas[y].shipping_speciality_parent_id) { 
			%>		
				<option value ="<%= datas[y].shipping_speciality_ID %>">--- <%= datas[y].shipping_speciality_name %></option>
			<% 
			}
			%>			
		<% 
		}//for
		%>		
   <% 
   } //if parrent 0
   %>
<% 
} //for all
%>
